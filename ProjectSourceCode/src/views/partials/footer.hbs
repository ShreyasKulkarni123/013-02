        fetch('/graphs_raw').then(res => {
            res.json().then(obj => {
                console.log(obj);
            })
        })

        new Chart("myChart", {
            type: "line",
            data: {
                labels: xValues,
                datasets: [{
                    backgroundColor: "rgba(0,0,255,1.0)",
                    borderColor: "rgba(0,0,255,0.1)",
                    data: yValues
                }]
            },
            options: { }
        });
